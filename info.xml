<?xml version="1.0"?>
<extension key="vitiligotheme" type="module">
  <file>vitiligotheme</file>
  <name>Vitiligo Society Theme</name>
  <description>This helps theme various CiviCRM forms to fit with the theme.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott / Artful Robot</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/vitiligosociety/vitiligotheme</url>
    <url desc="Support">https://artfulrobot.uk</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-05-21</releaseDate>
  <version>1.9</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.73</ver>
  </compatibility>
  <comments>This is a custom module for the Vitiligo Society</comments>
  <civix>
    <namespace>CRM/Vitiligotheme</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
